# Frontend (Next.js) environment example
# Development: NEXT_PUBLIC_API_URL can point to proxy (/api) or direct backend URL
NEXT_PUBLIC_API_URL=http://localhost:4000/api

# Optional: if using Supabase from frontend (anon key only)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Other public configs
NEXT_PUBLIC_APP_NAME=Kasir App
NEXT_PUBLIC_FEATURE_FLAG_EXAMPLE=false
